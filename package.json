    "axios": "^1.6.7",
    "focus-trap-react": "^11.0.4",
    "lucide-react": "^0.320.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.22.3",
