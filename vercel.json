{
  "name": "legal-chat-maxim-starykh",
  "version": 2,
  "builds": [
    {
      "src": "api/chat.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/chat"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
